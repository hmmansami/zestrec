generator client {
  provider = "prisma-client"
  output   = "client"
}

datasource db {
  provider = "postgresql"
}

model stores {
  shop     String   @id
  isActive Boolean? @default(false)

  @@index([shop])
}

model session {
  id      String  @id
  content String? @db.Text
  shop    String?

  @@index([id])
  @@index([shop])
}
